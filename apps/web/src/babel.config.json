{
  "parserOpts": {
    "sourceType": "module",
    "plugins": ["typescript"] // 启用 TS 语法解析（不剥离类型）
  },
  "presets": [
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "targets": {
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1"
        },
        "useBuiltIns": "usage",
        "corejs": "3.6.5"
      }
    ]
  ],
  "plugins": ["./src/babel/replace-console.ts"]
}
