{
  "name": "@myself/core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview"
  },
  "imports": {
    "#/*": "./src/*"
  },
  "files": [
    "src/"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./styles": {
      "import": "./src/assets/scss/index.scss",
      "default": "./src/assets/scss/index.scss"
    }
  },
  "dependencies": {
    "@ctrl/tinycolor": "catalog:",
    "@iconify/vue": "catalog:",
    "@myself/locales": "workspace:^",
    "@myself/store": "workspace:^",
    "@myself/utils": "workspace:^",
    "element-plus": "catalog:",
    "pinia": "catalog:",
    "screenfull": "catalog:",
    "theme-colors": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:"
  },
  "devDependencies": {
    "@myself/types": "workspace:^",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "@vue/tsconfig": "catalog:",
    "sass": "catalog:",
    "typescript": "catalog:",
    "unplugin-auto-import": "catalog:",
    "unplugin-vue-components": "catalog:",
    "vite": "catalog:",
    "vite-plugin-svg-icons": "catalog:",
    "vue-tsc": "catalog:"
  }
}