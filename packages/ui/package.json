{
  "name": "@myself/ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "gulp -f build-script/gulpfile.ts",
    "build:old": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "clean": "rimraf myself",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "imports": {
    "#/*": "./src/*"
  },
  "files": [
    "src/"
  ],
  "exports": {
    ".": {
      "types": "./myself/es/index.d.ts",
      "import": "./myself/es/index.mjs",
      "require": "./myself/"
    },
    "./es/*": {
      "types": "./myself/es/index.d.ts",
      "import": "./myself/es/components/*/index.mjs"
    },
    "./styles/*": {
      "import": "./myself/es/components/*/index.css",
      "default": "./src/assets/scss/index.scss"
    }
  },
  "sideEffects": [
    "build/*",
    "build/**/*.css",
    "build/**/*.scss",
    "@myself/utils/**/*.ts"
  ],
  "dependencies": {
    "@myself/utils": "workspace:^",
    "element-plus": "catalog:",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.18",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "@vue/tsconfig": "^0.8.1",
    "chalk": "^5.6.2",
    "child_process": "^1.0.2",
    "consola": "^3.4.2",
    "esbuild": "^0.25.12",
    "gulp": "^5.0.1",
    "rollup": "^4.52.5",
    "rollup-plugin-esbuild": "^6.2.1",
    "sass": "catalog:",
    "sucrase": "^3.35.0",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "catalog:",
    "unplugin-vue-components": "catalog:",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "2.0.0-alpha.12",
    "vue-tsc": "^3.1.0"
  }
}