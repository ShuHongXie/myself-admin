{
  "name": "@minilo/ui",
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tasks": "gulp -f build-script/gulpfile.ts --tasks",
    "test": "vitest --no-watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:gulp": "gulp --require ./node_modules/tsx/dist/loader.cjs -f ./gulpfile.ts",
    "build": "gulp --require ./node_modules/tsx/dist/loader.cjs -f ./build-script/gulpfile.ts",
    "gulp": "npx ts-node --require ts-node/register node_modules/gulp/bin/gulp.js -f build-script/gulpfile.ts",
    "build:old": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "clean": "rimraf minilo",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs --mode production",
    "docs:preview": "vitepress preview docs"
  },
  "imports": {
    "#/*": "./src/*"
  },
  "files": [
    "minilo/",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./minilo/es/index.d.ts",
      "import": "./minilo/dist/index.full.min.mjs",
      "require": "./minilo/dist/index.full.min.mjs"
    },
    "./resolver": {
      "types": "./minilo/es/resolver/index.d.ts",
      "import": "./minilo/es/resolver/index.mjs",
      "require": "./minilo/es/resolver/index.js"
    },
    "./es/*": {
      "types": "./minilo/es/index.d.ts",
      "import": "./minilo/es/components/*/index.mjs"
    },
    "./styles": {
      "import": "./minilo/dist/index.css",
      "default": "./minilo/dist/index.css"
    },
    "./theme-chalk/*": {
      "import": "./minilo/theme-chalk/*.css",
      "default": "./minilo/theme-chalk/*.css"
    },
    "./components/*": {
      "import": "./minilo/es/components/*",
      "default": "./minilo/es/components/*"
    }
  },
  "sideEffects": [
    "build/*",
    "build/**/*.css",
    "build/**/*.scss",
    "@minilo/utils/**/*.ts"
  ],
  "dependencies": {
    "@element-plus/icons-vue": "catalog:",
    "@iconify/vue": "catalog:",
    "@vueuse/core": "13.9.0",
    "element-plus": "catalog:",
    "vue": "catalog:",
    "vue-cropper": "^1.1.4"
  },
  "devDependencies": {
    "@minilo/utils": "workspace:^",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/gulp": "^4.0.18",
    "@types/gulp-rename": "^2.0.7",
    "@types/gulp-sass": "^6.0.0",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "axios-mock-adapter": "^1.22.0",
    "chalk": "^5.6.2",
    "child_process": "^1.0.2",
    "consola": "^3.4.2",
    "cssnano": "^7.1.2",
    "echarts": "^6.0.0",
    "esbuild": "^0.25.12",
    "fs-extra": "^11.3.2",
    "gh-pages": "^6.3.0",
    "gulp": "^5.0.1",
    "gulp-autoprefixer": "^10.0.0",
    "gulp-rename": "^2.1.0",
    "gulp-sass": "^6.0.1",
    "happy-dom": "^20.0.11",
    "jsdom": "^27.3.0",
    "postcss": "^8.5.6",
    "rollup": "^4.52.5",
    "rollup-plugin-esbuild": "^6.2.1",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "sass": "catalog:",
    "sucrase": "^3.35.0",
    "ts-node": "^10.9.2",
    "tsx": "4.19.3",
    "typescript": "~5.9.3",
    "unplugin-auto-import": "catalog:",
    "unplugin-vue-components": "catalog:",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "2.0.0-alpha.12",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.1.0"
  },
  "peerDependencies": {
    "vue": "^3.5.17"
  },
  "pnpm": {
    "overrides": {
      "@vueuse/core": "13.9.0",
      "@vueuse/shared": "13.9.0"
    }
  }
}