{
  "name": "admin",
  "version": "1.0.0",
  "description": "后台管理系统",
  "main": "index.js",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build": "turbo run build",
    "dev:web": "pnpm --filter @myself/web dev",
    "gen": "pnpm --filter @myself/web openapi-ts",
    "dev:server": "pnpm --filter @myself/server start:dev",
    "dev:ui": "pnpm --filter @myself/web dev",
    "lint": "eslint --cache \"**/*.{ts,js,vue}\" --fix",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,js,vue}\"",
    "lint:lint-staged": "lint-staged",
    "cz": "cz"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "pnpm lint",
      "pnpm format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-custom"
    }
  },
  "author": {
    "name": "xieshuhong",
    "url": "https://github.com/ShuHongXie/myself-admin"
  },
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/css": "^0.10.0",
    "@eslint/js": "^9.31.0",
    "@eslint/json": "^0.13.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/tsconfig": "catalog:",
    "commitizen": "^4.3.1",
    "cz-custom": "^0.0.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "3.6.2",
    "turbo": "^2.5.5",
    "typescript": "catalog:",
    "typescript-eslint": "^8.37.0",
    "vite": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  },
  "dependencies": {
    "axios": "catalog:"
  }
}