{
  "name": "admin",
  "version": "1.0.0",
  "description": "基础模板",
  "main": "index.js",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "build:old": "turbo run build",
    "dev:web": "pnpm --filter @minilo/web dev",
    "gen": "pnpm --filter @minilo/web openapi-ts",
    "analyze": "pnpm --filter @minilo/web analyze",
    "dev:server": "pnpm --filter @minilo/server start:dev",
    "dev:ui": "pnpm --filter @minilo/ui dev",
    "build:ui": "pnpm --filter @minilo/ui build",
    "lint": "eslint --cache \"**/*.{ts,js,vue}\" --fix",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,js,vue}\"",
    "lint:lint-staged": "lint-staged",
    "cz": "cz",
    "build": "pnpm --filter=@minilo/* run build",
    "clear": "rimraf 'packages/*/{lib,node_modules}' && rimraf node_modules",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish --registry=https://registry.npmjs.com/ --access=public",
    "docs:dev": "pnpm --filter @minilo/ui docs:dev",
    "docs:build": "pnpm --filter @minilo/ui docs:build",
    "docs:preview": "pnpm --filter @minilo/ui docs:preview"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": [
      "pnpm lint",
      "pnpm format"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-custom"
    }
  },
  "author": {
    "name": "xieshuhong",
    "url": "https://github.com/ShuHongXie/minilo"
  },
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/css": "^0.10.0",
    "@eslint/js": "^9.31.0",
    "@eslint/json": "^0.13.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitejs/plugin-vue": "catalog:",
    "@vue/tsconfig": "catalog:",
    "commitizen": "^4.3.1",
    "cz-custom": "^0.0.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "3.6.2",
    "turbo": "^2.5.5",
    "typescript": "catalog:",
    "typescript-eslint": "^8.37.0",
    "vite": "catalog:",
    "vue": "catalog:",
    "vue-tsc": "catalog:"
  },
  "dependencies": {
    "axios": "catalog:"
  },
  "pnpm": {
    "overrides": {
      "@minilo/ui": "^0.0.2",
      "@minilo/utils": "^0.0.2"
    }
  }
}